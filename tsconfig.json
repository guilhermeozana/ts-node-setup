  "compilerOptions": {
    "target": "es2016",                     
    "module": "commonjs",        
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "rootDir": "src",
    "outDir": "./dist",
    "moduleResolution": "node",
    "baseUrl": "src",
    "skipLibCheck": true
  }
}
